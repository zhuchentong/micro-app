    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@iconify-json/icon-park-outline": "^1.1.6",
